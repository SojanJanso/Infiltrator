; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Infiltrator"
run/main_scene="res://Scenes/Lobby.tscn"
boot_splash/image="res://Splash.png"
config/icon="res://icon.png"

[autoload]

Globals="*res://Scripts/Globals.gd"
Arrays="*res://Scripts/Arrays.gd"

[debug]

settings/fps/force_fps=30
gdscript/warnings/enable=false

[display]

window/size/width=360
window/size/height=640
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/physics_fps=30

[rendering]

quality/driver/driver_name="GLES2"
quality/intended_usage/framebuffer_allocation=0
vram_compression/import_etc=true
quality/shadows/filter_mode=0
environment/default_clear_color=Color( 0.20961, 0.213177, 0.222656, 1 )
environment/default_environment="res://default_env.tres"
